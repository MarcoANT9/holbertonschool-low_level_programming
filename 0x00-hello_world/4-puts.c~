#include <stdio.h>
#include <string.h>

/**
 * main - Print a string text
 *
 * Description: This C program will always print the string "Programming is like
 * building a multilingual puzzle" followed by a new line
 *
 * Return: - Always 0 (Success)
 */

int main(void)
{

	char string[50];

	strcpy(str, "Programming is like building a multilingual puzzle");
	puts(string);
	return (0);

}
