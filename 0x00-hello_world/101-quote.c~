#include <stdio.h>
#include <string.h>
#include <unistd.h>

/**
 * main - prints a text to the standard error
 *
 *
 * Description - Prints "and that piece of art is usefull" from Dora Korpar
 * using the "Write" command.
 *
 *
 *
 * Return: Always fails (1)
 *
 */


int main(void)
{

	int l_text;
	char text_w[] = "and that piece of art is useful\" - Dora Korpar,\
2015-10-19\n";

	scanf("%d", &l_text);

	write(2, text_w, l_text);

	return (1);

}
